{
  "name": "aw-ui-lerna",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "create": "plop",
    "pub": "lerna publish",
    "pub:force": "lerna publish --force-publish *",
    "pub:skip": "lerna publish --skip-git",
    "git": "git add -A && git commit -m 'deploy' && git push"
  },
  "devDependencies": {
    "lerna": "^5.4.3",
    "plop": "^3.1.1"
  },
  "dependencies": {
    "@rollup/plugin-babel": "^5.3.1",
    "rollup-plugin-postcss": "^4.0.2"
  }
}
